<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.memmo.notepad">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Notepad">
        <activity
            android:name="com.memmo.notepad.Text"
            android:exported="true" />
        <activity android:name="com.memmo.notepad.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.memmo.notepad.font_functions"></activity>
        <activity android:name="com.memmo.notepad.Background"></activity>
        <activity android:name="com.memmo.notepad.NoteActivity"></activity>

    </application>


</manifest>